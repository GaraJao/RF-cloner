  lerDados();
  


  
}


int menuPosition(int len) {
  int n = len-1;
  if(digitalRead(rightButton)==0){
    delay(200);
    iPosition++;
  }else if(digitalRead(leftButton)==0){
    delay(200);
    iPosition--;
  }
  if(iPosition>n){
    iPosition=0;
  }else if(iPosition<0){
    iPosition=n;
  }

}

bool enterStats() {
  if(digitalRead(enterButton) == 0){
  delay(250);
  enterPressed = true;
  return enterPressed;
  }

}

